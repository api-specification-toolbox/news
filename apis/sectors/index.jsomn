---
layout: none
---
[{% assign sectors = site.sectors %}{% assign sectors = sectors | sort: 'name' %}{% for sector in sectors %}
{
   "name": "{{ sector.name | strip_html }}",
   "description": "{{ sector.description | strip_html }}",
   "image": "{{ sector.image }}",
   "tags": {{ sector.tags | jsonify }},
   "created": "{{ sector.created }}",
   "modified": "{{ sector.modified }}",
   "url": "{{ sector.url }}",
   "sectorVersion": "{{ sector.sectorVersion }}",
   "apis": {{ sector.apis | jsonify }},
   "x-common": {{ sector.x-common | jsonify }}
}{% if forloop.last == true %}{% else %},{% endif %}
{% endfor %}]
